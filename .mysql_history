users
SELECT alias, name, surname, status FROM users;
SELECT User FROM mysql.user;
SHOW GRANTS FOR 'zabbix'@'localhost';
ALTER USER 'nome_do_usuario'@'localhost' IDENTIFIED WITH mysql_native_password BY 'nova_senha';
ALTER USER 'zabbix'@'localhost' IDENTIFIED WITH mysql_native_passowrd BY '1234';
SET PASSOWRD FOR 'zabbix'@'localhost' = PASSWORD('1234');
